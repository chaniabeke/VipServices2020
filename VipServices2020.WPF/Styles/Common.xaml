<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:VipServices2020.WPF.Styles">
    
    <!--Colors-->
    <Color x:Key="ColorPrimaryBlack">#0B0C10</Color>
    <Color x:Key="ColorPrimaryAccentBlue">#66FCF1</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimaryBlack" Color="{StaticResource ColorPrimaryBlack}"/>
    <SolidColorBrush x:Key="BrushPrimaryAccentBlue" Color="{StaticResource ColorPrimaryAccentBlue}"/>
    
</ResourceDictionary>